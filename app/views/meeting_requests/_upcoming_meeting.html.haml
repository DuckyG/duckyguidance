.request{:id =>[meeting.class, meeting.id],:class => request == last && "last"}
  .name.grid_11
    %h3=meeting.first_name + ' ' + meeting.last_name
  .clear
  .date
    .grid_2 
      %strong Date
    .grid_4
      =meeting.desired_date.strftime('%B %d @ %I:%M %p')
  .clear
  .notes
    .grid_1
      %strong Notes
    .clear
    %pre
      =meeting.notes
    .clear
  .clear
