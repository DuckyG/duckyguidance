.meeting{:id =>[meeting.class, meeting.id],:class => meeting == last && "last"}
  .meeting_info{:class =>odd_even}
    .grid_3
      %h6=meeting.created_at.strftime('%B %d @ %I:%M %p')
    .grid_3
      %strong=meeting.summary
    .grid_1.counselor.right
      =meeting.counselor.last_name
    .grid_1.right
      %strong Counselor
    .clear
  .clear
  =render :partial => 'shared/meeting_details', :locals => { :meeting => meeting}
  .clear