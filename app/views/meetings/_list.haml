-meeting_list.each do |meeting|
  .meeting_info{:class =>cycle('odd','even')}
    .grid_3
      %h6=meeting.occured_on.strftime('%B %d @ %I:%M %p') if meeting.occured_on
    .grid_5
      %strong=meeting.summary
    .grid_2.counselor.right
      =link_to meeting.student.last_name + ', ' +meeting.student.first_name, meeting.student
    .grid_1.right
      %strong Student
    .clear
  .clear
  